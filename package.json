{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm db:generate && next dev",
    "dev:pg": "DB=pg pnpm db:generate && next dev",
    "build": "pnpm db:generate && next build",
    "build:pg": "DB=pg pnpm db:generate && next build",
    "postinstall": "prisma generate --schema=prisma/schema.prisma",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "prisma generate --schema=prisma/schema.${DB:-sqlite}.prisma",
    "db:push": "prisma db push --schema=prisma/schema.${DB:-sqlite}.prisma"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.1",
    "@auth/prisma-adapter": "^2.11.1",
    "@base-ui-components/react": "1.0.0-rc.0",
    "@base-ui/react": "^1.0.0",
    "@extractus/article-extractor": "^8.0.20",
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-slot": "^1.2.4",
    "ai": "^6.0.3",
    "better-sqlite3": "^12.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.1",
    "next-auth": "5.0.0-beta.30",
    "prisma": "^6.19.1",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@tailwindcss/typography": "^0.5.19",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "prisma",
      "@prisma/engines",
      "@prisma/client"
    ]
  }
}
